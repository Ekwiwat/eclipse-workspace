package com.ekwiwat.javaPart1Ex3;
class exercise3 {
	public static void main(String[] args) {
//	ข้อ 3.1 a 
	int i = 20;
	i++;
	System.out.println(i);
	i++;
	System.out.println(i);
	i++;
	System.out.println(i);
	i++;
	System.out.println(i);
	i++;
	System.out.println(i);
	
//	ข้อ 3.1 b
	--i;
	System.out.println(i);
	--i;
	System.out.println(i);
	--i;
	System.out.println(i);
	--i;
	System.out.println(i);
	--i;
	System.out.println(i);
	
	}
}
